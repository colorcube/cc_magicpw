{
  "name": "colorcube/cc-magicpw",
  "description": "Magic Password Authentication TYPO3 Extension",
  "type": "typo3-cms-extension",
  "keywords": [
    "TYPO3",
    "extension",
    "password",
    "authentication",
    "service"
  ],
  "license": "GPL-2.0-or-later",
  "homepage": "https://github.com/colorcube/cc_magicpw",
  "support": {
    "issues": "https://github.com/colorcube/cc_magicpw/issues"
  },
  "authors": [
    {
      "name": "Ren√© Fritz",
      "email": "r.fritz@colorcube.de",
      "role": "Developer",
      "homepage": "http://www.colorcube.de"
    }
  ],
  "require": {
    "typo3/cms-core": "^6.2 || ^7.0 || ^8.0 || ^9.2.0"
  },
  "autoload": {
    "psr-4": {
      "Colorcube\\CcMagicpw\\": "Classes"
    }
  },
  "replace": {
    "cc-magicpw": "self.version",
    "typo3-ter/cc-magicpw": "self.version"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "cc_magicpw"
    }
  }
}
